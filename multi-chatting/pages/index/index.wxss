/**index.wxss**/
@import "../../font.wxss";
.container{
  height: 1500rpx;
}
.container .search{
  width: 735rpx;
  height: 65rpx;
  padding: 12.5rpx 0 12.5rpx 15rpx;
  background: rgb(24, 100, 20);
  color: rgb(24, 151, 20)D;
}
.container .search-left{
  flex: 8;
  background: rgb(255, 255, 255);
  text-align: left;
}
.container .search-left input{
  display: inline-block;
  height: 65rpx;
  font-size: 26rpx;
}
.search-placeholder{
  color: rgb(85, 86, 87);
  line-height: 20rpx;
}
.container .search .search-left image{
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  padding: 15rpx 15rpx 15rpx 20rpx;
}
.container .search .search-right{
  flex: 1;
}
.container .search .search-right image{
  width: 45rpx;
  height: 45rpx;
  padding: 10rpx;
}

.container{
  padding: 0;
  font-size: 14rpx;
  background: #F0F4F3;
  color: #000;
}

.mylabel {
    display: inline-block;
    height: 35rpx;
    line-height: 30rpx;
    padding: 10 15rpx;
    margin: 0 8rpx;
    font-family: PingFangSC;
    border-radius: 2px;
    vertical-align: middle;
    overflow: hidden;
    background: rgb(215, 235, 196);
    color: rgb(144, 148, 144);

    
  }
.label_text{
  
  border: 2px;
  font-size: 18rpx;
}

page{
  display: flex;
  flex-direction: column;
  height: 100%;
}
.navbar{
  flex: none;
  display: flex;
  background: #fff;
  margin: 0;
}
.navbar .item{
  position: relative;
  flex: auto;
  text-align: center;
  line-height: 80rpx;
}
.navbar .item.active{
  color: #FFCC00;
}

/*feed-item part is in app.wxss for multiplexing*/